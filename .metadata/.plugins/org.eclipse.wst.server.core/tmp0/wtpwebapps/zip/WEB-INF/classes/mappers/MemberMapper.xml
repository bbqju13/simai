<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myweb.www.repository.MemberDAO">
	<insert id="insert">
		insert into member(id, pw, name, birth, age, email, addr)
		values(#{id}, #{pw}, #{name}, #{birth}, #{age}, #{email}, #{addr})
	</insert>
	<select id="getUser" resultType="com.myweb.www.domain.MemberVO">
		select*from member where id = #{id}
	</select>
</mapper>